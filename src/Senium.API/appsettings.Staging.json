{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "server=@DB_HOST@;port=3306;user=@DB_USER@;password=@DB_PASS@;database=@DB_NAME@",
    "RedisConnection": "10.50.12.30:26379"
  },
  "AppSettings": {
    "UrlComum": "https://senium-h.maracanau.ifce.edu.br",
    "UrlGestao": "https://gestao.senium-h.maracanau.ifce.edu.br",
    "UrlApi": "https://api.senium-h.maracanau.ifce.edu.br"
  },
  "JwtSettings": {
    "ExpiracaoHoras": 6,
    "Emissor": "https://api.senium-h.maracanau.ifce.edu.br",
    "ComumValidoEm": "https://senium-h.maracanau.ifce.edu.br",
    "GestaoValidoEm": "https://gestao.senium-h.maracanau.ifce.edu.br",
    "CaminhoKeys": "/var/www/infra/Senium/keys"
  },
  "UploadSettings": {
    "PublicBasePath": "/var/www/src/Senium/uploads/public",
    "PrivateBasePath": "/var/www/src/Senium/uploads/private"
  },
  "EmailSettings": {
    "Name": "Senium Talentos",
    "User": "@EmailSettings_User@",
    "Password": "@EmailSettings_Password@",
    "Server": "@EmailSettings_Server@",
    "Port": 587
  }
}
